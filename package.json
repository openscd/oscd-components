{
  "name": "@openscd/oscd",
  "version": "0.0.1",
  "description": "OSCD Components",
  "main": "index.js",
  "type": "module",
  "workspaces": [
    "./components/*",
    "./lib/*",
    "./tools/*",
    "./storybook/*"
  ],
  "scripts": {
    "clean": "lerna run clean",
    "build": "lerna run build",
    "build:all": "nx run-many -t build --all --skip-nx-cache --nx-bail",
    "prestorybook": "npm run analyze",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate:component": "nx generate oscd:component",
    "component:status": "nx generate oscd:status",
    "analyze": "cem analyze",
    "test": "lerna run test --concurrency 1",
    "test:update": "lerna run test:update --concurrency 1",
    "predocumentation": "lerna run analyze",
    "documentation": "lerna run component:readme && lerna run readme"
  },
  "keywords": [
    "OpenSCD",
    "OSCD",
    "Components",
    "Oscd-Components"
  ],
  "author": {
    "name": "OpenSCD"
  },
